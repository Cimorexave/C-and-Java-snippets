		Book myBookB = new Book("title", "Author"); //gets author and title
		Book myBookC = new Book("title", "author", "releaseDate", "ISBN");
		Book myBookD = new Book("title", "author", "releaseDate", "ISBN" , "genre" , "comment");
	}
}


